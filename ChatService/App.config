<?xml version="1.0" encoding="utf-8" ?>
<configuration>
    <startup> 
        <supportedRuntime version="v4.0" sku=".NETFramework,Version=v4.7.2" />
    </startup>
    <system.serviceModel>
        <behaviors>
            <serviceBehaviors>
                <behavior name="">
                    <serviceMetadata httpGetEnabled="true" httpsGetEnabled="true" />
                    <serviceDebug includeExceptionDetailInFaults="true" />
                </behavior>
            </serviceBehaviors>
        </behaviors>

        <services>
            <service name="ChatService.Chat">
                <host>
                    <baseAddresses>
                        <add baseAddress="http://localhost:1230/Chat"/>
                    </baseAddresses>
                </host>
                <!--Service Endpoint-->
                <endpoint address="" binding="basicHttpBinding"
                          bindingConfiguration="" contract="ChatLib.IChat" />
            </service>
        </services>
    </system.serviceModel>
</configuration>